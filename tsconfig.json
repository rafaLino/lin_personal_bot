{
  "compilerOptions": {
    "target": "esnext",
    "module": "NodeNext",
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["src", "api"],
  "exclude": [
    "node_modules",
    "src/**/*.test.ts",
    "src/config"
  ],
  
}